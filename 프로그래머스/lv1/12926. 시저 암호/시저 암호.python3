def solution(s, n):
    answer = ''
    for i in s :
        if i == ' ': 
            answer += ' '
        elif 'A'<= i <= 'Z':
            answer += chr((ord(i)-65 + n)%26 + 65)
        elif 'a'<= i <= 'z':
            answer += chr((ord(i)-97 + n)%26 + 97)
            
            
    return answer